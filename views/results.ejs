<% include partials/header %>

<form action="/search">
  <input type="text" id = "keyword" value ="<%=keyword%>" placeholder="Type keyword" name="keyword">
  <button>
    Search
  </button>
  <a id = "favLink" href = "/displayKeywords"> See Favroties</a>
</form>


<% imageURLs.forEach(function(url, index) { %>
  <%- (index % 3 == 0) ? '<br>' : '' %>
<div class= "imageContainer">
  
  <img class = "image" src='<%=url%>' width='200' height='200'>
  <img class='favIcon' src='img/favorite.png' width='20'>
</div>
  <% }) %>

<% include partials/footer %>